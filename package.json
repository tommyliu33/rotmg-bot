{
	"name": "rotmg-bot",
	"version": "1.0.0-dev",
	"main": "index.js",
	"license": "UNLICENSED",
	"scripts": {
		"prepare": "husky install",
		"lint": "eslint src --ext ts .",
		"lint:fix": "eslint src --ext ts . --fix",
		"start": "tsup && cd dist && node -r source-map-support/register --experimental-specifier-resolution=node .",
		"start:deploy": "tsup && cd dist && node -r source-map-support/register --experimental-specifier-resolution=node . --deploy"
	},
	"imports": {
		"#struct/*": "./dist/lib/struct/*.js",
		"#functions/*": "./dist/functions/*.js",
		"#util/*": "./dist/lib/util/*.js",
		"#constants/*": "./dist/lib/util/constants/*.js",
		"#entities/*": "./dist/lib/entities/*.js",
		"#raids/": "./dist/src/util/raids/*.js"
	},
	"dependencies": {
		"@discordjs/builders": "^0.15.0",
		"@discordjs/rest": "^0.5.0",
		"@ltd/j-toml": "^1.30.0",
		"@sapphire/stopwatch": "^1.4.1",
		"@sapphire/utilities": "^3.6.2",
		"@toommyliu/realmeye-scraper": "github:toommyliu/realmeye-scraper",
		"common-tags": "^1.8.2",
		"discord-api-types": "^0.32.0",
		"discord.js": "^14.0.0-dev.1656060492-e24970e",
		"dotenv": "^16.0.1",
		"nanoid": "^4.0.0",
		"petitio": "^1.4.0",
		"pino": "^7.10.0",
		"readdirp": "^3.6.0",
		"reflect-metadata": "^0.1.13",
		"source-map-support": "^0.5.21",
		"tsyringe": "^4.7.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.2.3",
		"@commitlint/config-angular": "^17.0.0",
		"@types/common-tags": "^1.8.1",
		"@types/node": "^18.0.0",
		"@typescript-eslint/eslint-plugin": "^5.19.0",
		"@typescript-eslint/parser": "^5.29.0",
		"eslint": "^8.13.0",
		"eslint-config-marine": "^9.3.2",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier": "^2.6.2",
		"tsup": "^6.1.2",
		"typescript": "^4.6.3"
	},
	"engines": {
		"node": ">=v16.9.x"
	},
	"type": "module"
}
