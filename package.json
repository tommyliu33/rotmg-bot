{
	"name": "rotmg-bot",
	"version": "1.0.0-dev",
	"main": "index.js",
	"license": "UNLICENSED",
	"scripts": {
		"prepare": "husky install",
		"lint": "eslint src --ext ts .",
		"lint:fix": "eslint src --ext ts . --fix",
		"start": "tsup && cd dist && node -r source-map-support/register --experimental-specifier-resolution=node .",
		"start:deploy": "tsup && cd dist && node -r source-map-support/register --experimental-specifier-resolution=node . --deploy",
		"prisma:pull": "npx prisma db pull",
		"prisma:pull-force": "npx prisma db pull --force",
		"prisma:generate": "npx prisma generate"
	},
	"imports": {
		"#struct/*": "./dist/lib/struct/*.js",
		"#functions/*": "./dist/functions/*.js",
		"#util/*": "./dist/lib/util/*.js",
		"#constants/*": "./dist/lib/util/constants/*.js",
		"#entities/*": "./dist/lib/entities/*.js"
	},
	"dependencies": {
		"@discordjs/builders": "^0.13.0-dev.1649505810-3c0bbac",
		"@discordjs/rest": "^0.4.1",
		"@ltd/j-toml": "^1.30.0",
		"@prisma/client": "^3.13.0",
		"@sapphire/stopwatch": "^1.4.1",
		"@sapphire/type": "^2.2.0",
		"@sapphire/utilities": "^3.6.2",
		"@toommyliu/realmeye-scraper": "github:toommyliu/realmeye-scraper",
		"common-tags": "^1.8.2",
		"discord-api-types": "^0.32.0",
		"discord.js": "^14.0.0-dev.1651493371-4ba0f56",
		"dotenv": "^16.0.0",
		"nanoid": "^3.3.2",
		"petitio": "^1.4.0",
		"pino": "^7.10.0",
		"readdirp": "^3.6.0",
		"reflect-metadata": "^0.1.13",
		"source-map-support": "^0.5.21",
		"tsyringe": "^4.6.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.2.3",
		"@commitlint/config-angular": "^16.2.3",
		"@types/common-tags": "^1.8.1",
		"@types/node": "^17.0.23",
		"@typescript-eslint/eslint-plugin": "^5.25.0",
		"@typescript-eslint/parser": "^5.19.0",
		"eslint": "^8.13.0",
		"eslint-config-marine": "^9.3.2",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^7.0.4",
		"lint-staged": "^12.3.7",
		"prettier": "^2.6.2",
		"prisma": "^3.13.0",
		"tsup": "^5.12.4",
		"typescript": "^4.6.3"
	},
	"engines": {
		"node": ">=v16.9.x"
	},
	"type": "module"
}
