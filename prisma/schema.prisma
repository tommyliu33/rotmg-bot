generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model guilds {
  id                                String   @id @default(dbgenerated()) @map("_id") @db.ObjectId
  afk_check_channel_id              String
  cases                             Json[]
  guild_id                          String
  log_channel_id                    String
  main_section_id                   String
  main_section_requirements         Json
  main_section_voice_channel_ids    String[]
  main_verification_button_id       String
  raid_leader_role_id               String
  suspend_role_id                   String
  verified_role_id                  String
  vet_afk_check_channel_id          String
  vet_raid_leader_role_id           String
  veteran_role_id                   String
  veteran_section_id                String
  veteran_section_requirements      Json
  veteran_section_voice_channel_ids String[]
  veteran_verification_button_id    String
}

model users {
  id              String   @id @default(dbgenerated()) @map("_id") @db.ObjectId
  id_             String   @map("id")
  names           String[]
  stats           Json
  verified_guilds String[]
}
