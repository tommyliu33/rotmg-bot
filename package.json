{
	"name": "rotmg-bot",
	"version": "1.0.0-dev",
	"main": "index.js",
	"license": "UNLICENSED",
	"scripts": {
		"prepare": "husky install",
		"lint": "eslint src --ext ts .",
		"lint:fix": "eslint src --ext ts . --fix",
		"start": "tsup && cd dist && node -r source-map-support/register --experimental-specifier-resolution=node .",
		"start:silent": "tsup --silent && cd dist && node -r source-map-support/register --experimental-specifier-resolution=node .",
		"deploy": "tsup && cd dist && node --experimental-specifier-resolution=node ./util/deploy"
	},
	"imports": {
		"#struct/*": "./dist/struct/*.js",
		"#functions/*": "./dist/functions/*.js",
		"#util/*": "./dist/util/*.js"
	},
	"dependencies": {
		"@discordjs/builders": "^0.15.0",
		"@discordjs/rest": "^0.5.0",
		"@ltd/j-toml": "^1.30.0",
		"@sapphire/stopwatch": "^1.4.1",
		"@sapphire/utilities": "^3.6.2",
		"@toommyliu/realmeye-scraper": "github:toommyliu/realmeye-scraper",
		"common-tags": "^1.8.2",
		"discord-api-types": "^0.35.0",
		"discord.js": "^14.0.0-dev.1657411900-f0b68d5",
		"dotenv": "^16.0.1",
		"mongodb": "^4.7.0",
		"nanoid": "^4.0.0",
		"petitio": "^1.4.0",
		"pino": "^8.1.0",
		"readdirp": "^3.6.0",
		"reflect-metadata": "^0.1.13",
		"source-map-support": "^0.5.21",
		"tsyringe": "^4.7.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.0.3",
		"@commitlint/config-angular": "^17.0.0",
		"@types/common-tags": "^1.8.1",
		"@types/node": "^18.0.0",
		"@typescript-eslint/eslint-plugin": "^5.29.0",
		"@typescript-eslint/parser": "^5.29.0",
		"eslint": "^8.18.0",
		"eslint-config-marine": "^9.3.2",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"tsup": "^6.1.2",
		"typescript": "^4.7.4"
	},
	"engines": {
		"node": ">=16.9.x || >=18"
	},
	"type": "module"
}
